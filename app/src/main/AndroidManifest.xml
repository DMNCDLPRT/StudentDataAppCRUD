        android:label="@string/app_name"
        android:roundIcon="@android:drawable/ic_menu_today"
        android:supportsRtl="true"
        android:theme="@style/Theme.StudentDataApp">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="activity_create"
            android:label="@string/app_name_create">

        </activity>

        <activity
            android:name=".activity_update"
            android:parentActivityName=".activity_update">

        </activity>

    </application>

</manifest>